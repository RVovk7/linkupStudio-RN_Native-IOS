{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "globalReturn": true,
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "plugins": [
    "import",
    "react",
    "jsx-a11y",
    "jest"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".json"
        ]
      }
    },
    "import/extensions": [
      ".js",
      ".jsx"
    ],
    "import/core-modules": [],
    "import/ignore": [
      "node_modules",
      "\\.(scss|css|less|hbs|svg|json)$"
    ],
    "propWrapperFunctions": [
      "forbidExtraProps",
      "exact",
      "Object.freeze"
    ]
  },
  "rules": {
      "react/jsx-indent": 0,
      "indent": 0,
     "object-curly-newline":0,
     "react/jsx-one-expression-per-line":0,
     "no-console":1,
     "max-len":0,
     "import/extensions":1,
     "no-tabs":0,
     "arrow-parens":0,
     "arrow-body-style":0,
     "no-undef":0,
     "react/jsx-filename-extension":0,
     "eol-last":0,
     "quote-props":0,
     "import/no-unresolved":0,
     "react/prefer-stateless-function":0,
     "react/jsx-indent-props":0,
     "react/no-unused-prop-types":0,
     "no-underscore-dangle":0,
     "no-unused-vars":1,
     "react/no-unused-state":0,
     "consistent-return":0,
     "one-var":0,
     "import/prefer-default-export":0,
     "no-return-assign":0,
     "react/require-default-props":0,
     "react/destructuring-assignment":0
  }
}



